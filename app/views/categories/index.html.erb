<h1>Listing Category</h1>

<table>
	<thead>
		<tr>
			<th>id</th>
			<th>name</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @categories.each do |category| %>
			<tr>
				<td><%= category.category_id %></td>
				<td><%= category.name %></td>
				<td><%= link_to 'Show', category %></td>
			</tr>
		<% end %>
	</tbody>
</table>